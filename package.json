    "db": "mongod --dbpath=db --rest",
    "lint:style": "stylelint './src/**/*.j*'",
    "lint:fix": "eslint ./src --ext .js,.jsx --fix || stylelint './src/**/*.css' --fix",
    "test": "eslint ./src --ext .js,.jsx",
    "precommit": "lint-staged"
  },
